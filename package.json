{
    "author": "Christian Florentz (christian.florentz@gmail.com)",
    "dependencies": {
        "body-parser": "^1.20.0",
        "byte-size": "^8.1.0",
        "cors": "^2.8.5",
        "express": "^4.18.1",
        "helmet": "^5.0.2",
        "multer": "^1.4.4",
        "serve-index": "^1.9.1",
        "uuid": "^8.3.2"
    },
    "description": "The unofficial Nord Stage 2/2EX/3 Program viewer tool.",
    "devDependencies": {
        "@types/jest": "^27.4.1",
        "better-docs": "^2.7.2",
        "concurrently": "^7.1.0",
        "coverage-badge-creator": "^1.0.11",
        "eslint": "^8.14.0",
        "jest": "^28.0.3",
        "jsdoc": "^3.6.10",
        "jsdoc-to-markdown": "^7.1.1",
        "markdown-magic": "^2.6.0",
        "node-pandoc": "^0.3.0",
        "nodemon": "^2.0.16",
        "prettier": "^2.6.2",
        "showdown": "^2.1.0",
        "unzipper": "^0.10.11",
        "xml-js": "^1.6.11"
    },
    "engines": {
        "node": "16.x"
    },
    "jest": {
        "modulePathIgnorePatterns": [
            "client"
        ],
        "coverageReporters": [
            "json-summary",
            "text",
            "text-summary"
        ],
        "verbose": true,
        "reporters": [
            "default",
            "./test/test-md-reporter.js"
        ]
    },
    "keywords": [
        "Nord",
        "Stage",
        "NS3",
        "File"
    ],
    "license": "MIT",
    "name": "ns-file-parser",
    "prettier": {},
    "repository": {
        "type": "git",
        "url": "https://github.com/Chris55/ns3-program-viewer.git"
    },
    "resolutions": {
        "pug": "^3.0.1"
    },
    "scripts": {
        "build": "NODE_ENV=production cd client && yarn install && yarn react-build",
        "client": "cd client && yarn react-start",
        "deploy:web": "heroku git:remote -a ns3-program-viewer && git push heroku master:main",
        "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
        "doc:lead": "./docs/build-doc.sh lead",
        "doc:stage": "./docs/build-doc.sh stage",
        "server": "nodemon server.js",
        "start": "NODE_ENV=production node server.js",
        "test": "jest --coverage && coverage-badge-creator"
    },
    "version": "1.5.5"
}
