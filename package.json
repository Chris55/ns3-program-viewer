        "ag-grid-react": "^28.0.0",
        "axios": "^0.27.2",
        "body-parser": "^1.20.0",
        "bootstrap": "^4.6.2",
        "byte-size": "^8.1.0",
        "classnames": "^2.3.1",
        "cors": "^2.8.5",
        "electron-is-dev": "^2.0.0",
        "electron-log": "^4.4.8",
        "electron-updater": "^5.2.1",
        "express": "^4.18.1",
        "express-rate-limit": "^6.5.1",
        "file-saver": "^2.0.5",
        "helmet": "^5.1.1",
        "html2canvas": "^1.4.1",
        "jspdf": "^2.5.1",
        "multer": "^1.4.5-lts.1",
