{
    "author": "Christian Florentz (christian.florentz@gmail.com)",
    "dependencies": {
        "body-parser": "^1.18.3",
        "byte-size": "^7.0.1",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "helmet": "^4.6.0",
        "multer": "^1.4.2",
        "serve-index": "^1.9.1",
        "uuid": "^8.3.0"
    },
    "description": "The unofficial Nord Stage 2/2EX/3 Program viewer tool.",
    "devDependencies": {
        "@types/jest": "^26.0.10",
        "better-docs": "^2.3.2",
        "concurrently": "^5.3.0",
        "eslint": "^6.6.0",
        "jest": "^24.9.0",
        "jest-coverage-badges": "^1.1.2",
        "jsdoc": "^3.6.7",
        "jsdoc-to-markdown": "^7.1.0",
        "markdown-magic": "^2.5.2",
        "node-pandoc": "^0.3.0",
        "nodemon": "^2.0.6",
        "prettier": "^2.1.1",
        "showdown": "^1.9.1",
        "unzipper": "^0.10.11",
        "xml-js": "^1.6.11"
    },
    "engines": {
        "node": "14.x"
    },
    "eslintConfig": {
        "root": true,
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
            "ecmaVersion": 2020,
            "sourceType": "module"
        },
        "extends": [
            "eslint:recommended",
            "plugin:mocha/recommended",
            "plugin:@typescript-eslint/recommended",
            "prettier/@typescript-eslint",
            "plugin:prettier/recommended"
        ],
        "env": {
            "commonjs": true,
            "es6": true,
            "node": true,
            "jest": true
        },
        "plugins": [
            "jest"
        ],
        "ignorePatterns": [
            "client/**/*.js"
        ]
    },
    "jest": {
        "modulePathIgnorePatterns": [
            "client"
        ],
        "coverageReporters": [
            "json-summary",
            "text",
            "lcov"
        ]
    },
    "keywords": [
        "Nord",
        "Stage",
        "NS3",
        "File"
    ],
    "license": "MIT",
    "name": "ns-file-parser",
    "prettier": {},
    "scripts": {
        "client": "cd client && yarn react-start",
        "server": "nodemon server.js",
        "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
        "start": "NODE_ENV=production node server.js",
        "build": "cd client && yarn install && yarn react-build",
        "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run react-build",
        "test": "jest --coverage && jest-coverage-badges",
        "doc:stage": "./docs/build-doc.sh stage",
        "doc:lead": "./docs/build-doc.sh lead"
    },
    "version": "1.0.0"
}
