        "react-bootstrap": "1.6.5",
        "react-dom": "^18.2.0",
        "react-icons": "^4.3.1",
        "react-json-tree": "^0.17.0",
        "react-redux": "^8.0.1",
        "react-router-dom": "^6.3.0",
        "react-scripts": "^5.0.1",
        "react-splitter-layout": "^4.0.0",
        "react-toastify": "^9.0.7",
