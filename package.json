{
    "author": "Christian Florentz (christian.florentz@gmail.com)",
    "dependencies": {
        "body-parser": "^1.19.2",
        "byte-size": "^8.1.0",
        "cors": "^2.8.5",
        "express": "^4.17.3",
        "helmet": "^5.0.2",
        "multer": "^1.4.4",
        "serve-index": "^1.9.1",
        "uuid": "^8.3.2"
    },
    "description": "The unofficial Nord Stage 2/2EX/3 Program viewer tool.",
    "devDependencies": {
        "@types/jest": "^27.4.1",
        "better-docs": "^2.7.2",
        "concurrently": "^7.1.0",
        "eslint": "^8.12.0",
        "jest": "^27.5.1",
        "jest-coverage-badges": "^1.1.2",
        "jsdoc": "^3.6.10",
        "jsdoc-to-markdown": "^7.1.1",
        "markdown-magic": "^2.6.0",
        "node-pandoc": "^0.3.0",
        "nodemon": "^2.0.15",
        "prettier": "^2.6.1",
        "showdown": "^2.0.3",
        "unzipper": "^0.10.11",
        "xml-js": "^1.6.11"
    },
    "engines": {
        "node": "14.x"
    },
    "eslintConfig": {
        "root": true,
        "extends": [
            "prettier"
        ]
    },
    "jest": {
        "modulePathIgnorePatterns": [
            "client"
        ],
        "coverageReporters": [
            "json-summary",
            "text",
            "lcov"
        ]
    },
    "keywords": [
        "Nord",
        "Stage",
        "NS3",
        "File"
    ],
    "license": "MIT",
    "name": "ns-file-parser",
    "prettier": {},
    "scripts": {
        "client": "cd client && yarn react-start",
        "server": "nodemon server.js",
        "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
        "start": "NODE_ENV=production node server.js",
        "build": "cd client && yarn install && yarn react-build",
        "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run react-build",
        "test": "jest --coverage && jest-coverage-badges",
        "doc:stage": "./docs/build-doc.sh stage",
        "doc:lead": "./docs/build-doc.sh lead"
    },
    "version": "1.5.3"
}
