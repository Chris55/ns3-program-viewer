{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "builds": [{ "src": "package.json", "use": "@vercel/node" }],
    "routes": [
        {
            "src": "/api/upload",
            "dest": "/build/server.min.js",
            "methods": ["POST"]
        },
        {
            "src": "/media",
            "dest": "/build/server.min.js",
            "methods": ["GET"]
        }
    ]
}
